#include <iostream>
#include <stdio.h>
using namespace std;
class Group{
    //
public:    
    int female;
    int male;
    int total();
    Group(int a = 0, int b=0){
        female = a;
        male = b;
    }
    ~Group(){cout <<"Group was denied\n";}
};

int Group::total(){
    return female+male;
    }
